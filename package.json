{
    "name": "lambda",
    "version": "1.0.0",
    "description": "Lambda calculus interpreter library and CLI.",
    "type": "commonjs",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
        "test": "mocha --require ts-node/register ./tests/**/*.test.ts",
        "build": "tsc -p .",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
        "clean": "rm -rf ./dist"
    },
    "bin": {
        "lambda": "dist/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/minchingtonak/lambdacalculus.git"
    },
    "keywords": [
        "lambda",
        "calculus",
        "functional"
    ],
    "author": "Alec Minchington",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/minchingtonak/lambda/issues"
    },
    "homepage": "https://github.com/minchingtonak/lambda#readme",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.13.5",
        "babel-core": "^6.26.3",
        "babel-register": "^6.26.0",
        "chai": "^4.2.0",
        "mocha": "^7.1.2",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3",
        "yarn": "^1.22.4",
        "nodemon": "^2.0.3"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    },
    "dependencies": {}
}
