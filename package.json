{
    "name": "lambster",
    "version": "2.0.0",
    "description": "Lambda calculus interpreter library and CLI.",
    "type": "commonjs",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "sideEffects": false,
    "scripts": {
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
        "test": "mocha --require ts-node/register ./tests/**/*.test.ts",
        "build": "genversion --es6 --semi src/version.ts && tsc -p .",
        "link": "rm ./env/bin/lambster && npm link --force",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
        "clean": "rm -rf ./dist"
    },
    "bin": {
        "lambster": "./dist/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/minchingtonak/lambster.git"
    },
    "keywords": [
        "lambda",
        "calculus",
        "functional",
        "interpreter",
        "programming",
        "language",
        "cli",
        "front-end",
        "frontend"
    ],
    "author": "Alec Minchington",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/minchingtonak/lambster/issues"
    },
    "homepage": "https://github.com/minchingtonak/lambster#readme",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.13.9",
        "babel-core": "^6.26.3",
        "babel-register": "^6.26.0",
        "chai": "^4.2.0",
        "genversion": "^2.2.1",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.3",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3",
        "yarn": "^1.22.4"
    },
    "dependencies": {},
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    }
}
